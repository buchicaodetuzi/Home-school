<template name="messagelist">
  <view class='messagedetail'>
    <image class='messagedetail-img' src='/images/message.png'></image>
    <image class='arrow-img' src='/images/arrow.png'></image>
    <view class='top-right'>
      <text class='title'>来自：{{sendName}}家长</text>
      <text class='brief-message'>{{content}}</text>
    </view>
    <view class="separate"></view>
    <text class='date'>{{time}}</text>
    <text class='read'>{{state}}</text>
  </view>
</template>